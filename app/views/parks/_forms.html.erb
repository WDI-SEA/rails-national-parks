<div class="container">
  <%= bootstrap_form_for(@park) do |f| %>
    <%= f.text_field :name %>

    <%= f.text_area :description %>

    <%= f.collection_check_boxes(:ranger_ids, @rangers, :id, :name) %>

    <%= f.text_field :url %>

    <%= f.primary %>
    <%= link_to('Back to Parks', parks_path, class: 'btn btn-success') %>
  <% end -%>

</div>