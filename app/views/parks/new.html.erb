<h1>Add your new park:</h1>
<%= form_for @park do |form| %>
  <%= form.label :name %>
  <br>
  <%= form.text_field :name %>
  <br><br>
  <%= form.label :Photo_Url %>
  <br>
  <%= form.text_field :picture %>
  <br><br>
  <%= form.label :description %>
  <br>
  <%= form.text_area :description %>
  <br><br>
  <%= form.submit %>
<% end %>