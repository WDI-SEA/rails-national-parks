<h1>Add a park</h1>
<h3>To add a park, just enter the name of the park, and a brief description about it.</h3>

<%= form_for @park do |t| %>
	<div>
		<%= t.label :name %>
		<%= t.text_field :name %>
	</div>

	<div>
		<%= t.label :description %>
		<%= t.text_area :description %>
	</div>

	<div>
	
	</div>

	<%= t.submit %>
	<% end %>