<h1>Parks#new</h1>
<p>Find me in app/views/parks/new.html.erb</p>

<h2>Create a new park</h2>

<%= form_for @park do |p| %>
	<div>
		<%= p.label :name %>
		<%= p.text_field :name %> 
	</div>
	<div>
		<%= p.label :description %>
		<%= p.text_area :description %>
	</div>
	<div>
		<%= p.label :picture %>
		<%= p.file_field :picture %>
	</div>
	<%= p.submit %>

<% end %>

<%= link_to "Back to all", "/parks" %>

