<h1 class="header"><%= @park.name %></h1>



<h3 class="header">Park Description</h3>
<div class="well">
	<p><%= @park.description %></p>
</div>

<h3 class="header">Picture Link</h3>
<div class="well">
	<p><%= @park.picture %> </p>
</div>

<%= render "application/partials/footer" %>
