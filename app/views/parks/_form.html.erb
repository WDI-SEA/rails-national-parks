<%= form_for(@park) do |f| %>
    <div>
    <%= f.label :name %>
    <%= f.text_area :name %>
    </div>
    <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
    </div>
    <div>
    <%= f.label :picture %>
    <%= f.file_field :picture %>
    </div>
    <%= f.submit %>
<% end %>
