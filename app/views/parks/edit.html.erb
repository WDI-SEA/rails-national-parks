<h1>Edit Park Information</h1>

<%= form_for @park do |p| %>
  <div>
    <%= p.label :name %>
    <%= p.text_field :name %>    
  </div>
  <div>
    <%= p.label :description %>
    <%= p.text_field :description %>
  </div>
  <div>
    <%= p.label :picture %>
    <%= p.text_field :picture %>
  </div>
  <%= p.submit %>

<% end %>

<%= link_to "Back to All Parks", parks_path %>